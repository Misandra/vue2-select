{
  "name": "@misandra/vue2-select",
  "version": "1.0.1",
  "description": "Vue.js select",
  "author": "Tatyana Zherebtsova <zherebtsova.ti@gmail.com>",
  "paths": {
    "source": "src",
    "destination": "dist"
  },
  "private": false,
  "main": "dist/vue2-select.js",
  "license": "MIT",
  "lint-staged": {
    "dist/**/*.{js,vue}": [
      "jest --findRelatedTests",
      "eslint --fix"
    ]
  },
  "scripts": {
    "prepare": "npm run build",
    "serve": "webpack-dev-server --config build/webpack.dev.config.js --hot --progress -d",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js --progress",
    "lint": "eslint --fix --ext .js,.vue dist",
    "lint:dev": "eslint --fix --ext .js,.vue dev",
    "husky": "npx husky install && npm run husky:commit",
    "husky:commit": "npx husky add .husky/pre-commit \"lint-staged\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Misandra/vue2-select"
  },
  "peerDependencies": {
    "vue": "2.x"
  },
  "keywords": [
    "vue",
    "select",
    "dropdown"
  ],
  "devDependencies": {
    "@babel/core": "~7.2",
    "@babel/polyfill": "~7.2",
    "@babel/preset-env": "~7.3",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/test-utils": "^1.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-array-includes": "^2.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^7.4.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-vue": "^7.0.0-alpha.9",
    "husky": "^6.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^4.10.0",
    "path": "^0.12.7",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "vue": "^2.6.8",
    "vue-jest": "^3.0.6",
    "vue-loader": "^15.7.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "projects": [
      {
        "displayName": "test",
        "moduleFileExtensions": [
          "vue",
          "js"
        ],
        "testMatch": [
          "<rootDir>/dist/**/*.spec.js"
        ],
        "transform": {
          ".*\\.(vue)$": "vue-jest",
          "^.+\\.(js|jsx)?$": "babel-jest"
        },
        "transformIgnorePatterns": [
          "<rootDir>/node_modules/"
        ],
        "moduleNameMapper": {
          "^@/(.*)$": "<rootDir>/dist/$1"
        },
        "collectCoverageFrom": [
          "<rootDir>/dist/**/*.{vue,js}"
        ]
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/Misandra/vue2-select/issues"
  },
  "homepage": "https://github.com/Misandra/vue2-select#readme"
}
